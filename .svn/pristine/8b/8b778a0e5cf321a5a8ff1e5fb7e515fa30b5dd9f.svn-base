package com.bsth.busDataOperate.util;

import com.ndktools.javamd5.core.MD5;

//MD532加密
public class Md532 {
	//MD532加密
	public static String md532encryption(String s) {
		System.out.println();
		String encryption=new MD5().getMD5ofStr(s).toLowerCase();
		System.out.println("ssss:"+s+"\tencryption:"+encryption);
		return encryption;
	}
}
